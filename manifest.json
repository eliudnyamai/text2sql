{
    "manifest_version": 3,
    "name": "Text to SQL Converter",
    "version": "1.0",
    "description": "Converts text to SQL for PHP, HTML, CSS, and JS.",
    
      "permissions": [
        "activeTab",
        "http://*/*",        
        "https://*/*"       
      ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  